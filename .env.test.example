# .env.test.example
# Environment variables for DevCle test environment
# Copy this file to .env.test and replace the placeholder values

# ============================================
# Database Configuration
# ============================================
POSTGRES_DB=devcle
POSTGRES_USER=devcle
POSTGRES_PASSWORD=changeme
DATABASE_HOST=127.0.0.1
DATABASE_PORT=55432
DATABASE_URL=postgresql://devcle:changeme@127.0.0.1:55432/devcle

# ============================================
# Redis Configuration
# ============================================
REDIS_PASSWORD=changeme
REDIS_URL=redis://:changeme@127.0.0.1:56379/0

# ============================================
# Application Configuration
# ============================================
NODE_ENV=test
SESSION_SECRET=changeme
ENCRYPTION_KEY=changeme
APP_DOMAIN=devcle.test
BASE_URL=https://devcle.test

